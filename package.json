{
  "name": "billboardjs-playground",
  "version": "0.7.0",
  "description": "playground for billboard.js",
  "main": "index.js",
  "scripts": {
    "start": "npm run extract:document && webpack-dev-server --open",
    "build": "npm run extract:document && webpack --env production --config ./webpack.production.config.js",
    "extract:document": "echo module.exports = > src/app/configure/resources/document.js && jsdoc2md --json node_modules/billboard.js/src/config/Options.js >> src/app/configure/resources/document.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/billboard.js-playground"
  },
  "keywords": [
    "billboard.js",
    "chart",
    "generator",
    "playground"
  ],
  "author": "joohwee.kim",
  "license": "MIT",
  "dependencies": {
    "billboard.js": "^1.2.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "create-react-class": "^15.6.2",
    "js-beautify": "^1.6.14",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-watcher": "0.0.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-config-naver": "^1.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "jsdoc-to-markdown": "^3.0.3",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "portscanner": "^2.1.1",
    "postcss": "^6.0.11",
    "postcss-cssnext": "^3.0.2",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "react-billboardjs": "git://github.com/kjhwee91/react-billboardjs.git",
