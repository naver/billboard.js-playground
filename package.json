    "sass-loader": "^6.0.6",
    "scss-loader": "0.0.1",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^2.1.1",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
